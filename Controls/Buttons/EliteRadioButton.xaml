<RadioButton x:Class="ODEliteTracker.Controls.Buttons.EliteRadioButton"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ODEliteTracker.Controls.Buttons"
             mc:Ignorable="d" 
             OverridesDefaultStyle="False"
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Height="100"
          Margin="20"
          Width="100">
        <Path Stroke="{DynamicResource PrimaryForeground}"
              Fill="{DynamicResource PrimaryForeground}"
              Stretch="Fill"
              Data="M0 0H5V1H1V5H0V0M10 0h5m0 0V5H14V1H10V0m5 10v5H10V14h4V10h1M5 15H0V10H1v4H5v1" />
        <Rectangle Fill="{DynamicResource PrimaryForeground}"
                   Margin="25" />
    </Grid>
</RadioButton>
