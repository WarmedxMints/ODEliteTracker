<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <Color x:Key="Overlay_CardBackGrounColour">#FF2D2D2D</Color>
    <SolidColorBrush x:Key="Overlay_CardBackground"
                     Color="{DynamicResource Overlay_CardBackGrounColour}" />

    <Color x:Key="Overlay_Border">DarkGray</Color>
    <SolidColorBrush x:Key="Overlay_BorderBrush"
                     Color="{DynamicResource Overlay_Border}" />

    <Color x:Key="Overlay_TitleBar">#FF202020</Color>
    <SolidColorBrush x:Key="Overlay_TitleBarBrush"
                     Color="{DynamicResource Overlay_TitleBar}" />

    <Color x:Key="Overlay_Foreground">#FFF3F3F3</Color>
    <SolidColorBrush x:Key="Overlay_ForegroundBrush"
                     Color="{DynamicResource Overlay_Foreground}" />
</ResourceDictionary>