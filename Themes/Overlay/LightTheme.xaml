<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="Overlay_CardBackGrounColour">#FFF4F4F4</Color>
    <SolidColorBrush x:Key="Overlay_CardBackground"
                     Color="{StaticResource Overlay_CardBackGrounColour}" />

    <Color x:Key="Overlay_Border">Black</Color>
    <SolidColorBrush x:Key="Overlay_BorderBrush"
                     Color="{StaticResource Overlay_Border}" />

    <Color x:Key="Overlay_TitleBar">#FFC7C7C7</Color>
    <SolidColorBrush x:Key="Overlay_TitleBarBrush"
                     Color="{StaticResource Overlay_TitleBar}" />

    <Color x:Key="Overlay_Foreground">Black</Color>
    <SolidColorBrush x:Key="Overlay_ForegroundBrush"
                     Color="{StaticResource Overlay_Foreground}" />
</ResourceDictionary>